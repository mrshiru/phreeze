* {ldelim}
	font-family: verdana,arial,helvetica; 
	font-size: 10pt; 
	color: #444444;
{rdelim}

img {ldelim}border: none;{rdelim}

p {ldelim}margin: 1em 0;{rdelim}
h1,h2,h3 {ldelim}margin: .75em 0;{rdelim}

h1 {ldelim}font-size: 14pt;{rdelim}

body
{ldelim}
	padding: 0px;
	background-color: #666666;
{rdelim}

#main
{ldelim}
	padding: 0px;
	margin: 0px;
	text-align: center; 
{rdelim}

#stage
{ldelim}
	width: 880px; 
	overflow: hidden;
	background-color: #ffffff;
	padding: 0px 15px 15px 15px;
	margin: auto;
	text-align: left;
	border-left: solid 3px #444444;
	border-right: solid 3px #444444;
{rdelim}

#nav
{ldelim}
	padding: 8px;
	margin-bottom: 10px;
	background-color: #dddddd;
	border: solid 1px #aaaaaa;
{rdelim}

#nav a
{ldelim}
	text-decoration: none;
	font-weight: bold;
{rdelim}

#nav a:hover
{ldelim}
	text-decoration: underline;
{rdelim}

#footer
{ldelim}
	padding: 8px 0px 8px 0px;
	margin-top: 10px;
	border-top: solid 1px #aaaaaa;
{rdelim}

#footer, #footer *
{ldelim}
	font-size: 9px;
{rdelim}

.grid_panel
{ldelim}
	width:600px; 
	height:300px; 
{rdelim}

/* ext feedback */ 
#feedback-div {ldelim}
    position:absolute;
    left:35%;
    top:10px;
    width:350px;
    z-index:20000;
{rdelim}

.field_validator
{ldelim}
	display: none;
	font-style: italic;
	color: Red;
{rdelim}

.form_validator
{ldelim}
	display: none;
	background-color: Red;
	color: #ffffff;
	padding: 3px;
	font-weight: bold;
{rdelim}

.info
{ldelim}
	background-color: #ffffff;
	color: #0000ff;
	padding: 3px;
	font-weight: bold;
	margin-bottom: 3px;
{rdelim}

.warning
{ldelim}
	background-color: #ff0000;
	color: #222222;
	padding: 3px;
	font-weight: bold;
	margin-bottom: 3px;
{rdelim}

